  res.header("Access-Control-Allow-Origin", "*");
    res.header('Access-Control-Allow-Methods', 'PUT, GET, POST, DELETE, OPTIONS');
    res.header("Access-Control-Allow-Headers", "X-Requested-With,token,xiaoben");   //设置这个，前端自定义的headee(token或xiaoben)才能生效
    res.header("Content-Type", "application/json;charset=utf-8");
	res.header("Access-Control-Expose-Headers","name");  //设置这个前端才能获取后端自定义header
